<?rev   put "index" into gControllerHandlers   startUsingLibraries    on index       put fedwiki_GetDroppedUrl ($_POST_RAW) into droppedURL       put mediwiki_ExtractPageTitleFromUrl (droppedURL) into pageTitle       fedwiki_ReturnError pageTitle, "Not a Wikipedia page!"              put fedwiki_FetchWikipediaPageJson (pageTitle) into someJSON       fedwiki_ReturnJSON someJSON    end index    command startUsingLibraries       start using stack (gAPPPATH & "stacks/" & lib_Fedwiki.livecode )       start using stack (gAPPPATH & "stacks/" & lib_MediaWiki.livecode )       start using stack (gAPPPATH & "stacks/" & lib_FedWikiPedia.livecode )    end startUsingLibraries?>